table|products|products|4|

CREATE TABLE "products" 
("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, 
 "title" varchar, 
 "description" text, 
 "image_url" varchar, 
 "price" decimal(8,2), 
 "created_at" datetime NOT NULL, 
 "updated_at" datetime NOT NULL, 
 BuyCount DECIMAL, 
 image_url2 varchar(50), 
 product_id decimal, 
 type varchar(10), 
 close_type varchar(10), 
 close_type2 string(10))
